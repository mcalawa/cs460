@model IEnumerable<final.Models.Casts>
@{
    ViewBag.Title = "Cast";
}

<h2>Cast</h2>
<table>
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.ActorId)</th>
            <th>@Html.DisplayNameFor(model => model.MovieId)</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(ModelItem => item.Actors.Name)</td>
                <td>@Html.DisplayFor(ModelItem => item.Movies.Title)</td>
            </tr>
        }
    </tbody>
</table>

